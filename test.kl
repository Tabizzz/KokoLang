.func sum
{
	.locals 3
	.stack 2
	// iterations
	push 1000000
	stvar 0
	// total
	push 0.0
	stvar 1
	// i
	push 0
	stvar 2
	
	:comp
	ldvar 2
	ldvar 0
	oplt
	goif end

	ldvar 1
	ldvar 2
	add
	stvar 1

	// i++;
	ldvar 2
	push 1
	add
	stvar 2
	go comp

	:end
	ldvar 1
	call out
	ret
}

.func main
{
	.locals 0
	.stack 1
	push "Hello world"
	call out
	call sum
}